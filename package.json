{
  "name": "tgfs",
  "version": "1.0.0",
  "description": "Use telegram as file storage",
  "main": "index.js",
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "build:dev": "tsc -p tsconfig.json",
    "start": "yarn build:dev && node dist/src/index.js",
    "start:dev": "yarn build:dev && nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/index.ts",
    "start:prod": "node dist/src/index.js",
    "start:cmd": "yarn build:dev && node dist/src/cmd.js",
    "start:cmd:prod": "node dist/src/cmd.js",
    "cmd": "yarn start:cmd:prod",
    "test": "yarn jest",
    "test:cov": "yarn jest --coverage"
  },
  "author": "Wheat Carrier",
  "license": "Apache-2.0",
  "dependencies": {
    "@types/jest": "^29.5.2",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "input": "^1.0.1",
    "telegram": "^2.17.10",
    "uuid": "^9.0.0",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^4.1.1",
    "@types/express": "^4.17.17",
    "@types/node": "^20.3.3",
    "@types/uuid": "^9.0.2",
    "@types/yargs": "^17.0.24",
    "jest": "^29.6.0",
    "module-alias": "^2.2.3",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.8",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  }
}
